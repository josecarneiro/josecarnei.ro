- const education = data.education;
section.resume__section.resume__section--education
  h2 Education
  each item in education
    div.listing__item
      header.listing__header
        .listing__row
          h3=item.school
          small=functions.formatDate([ item.date_start, item.date_end ])
        if item.description
          small=item.description
      h4=item.degree
      ul
        each highlight in item.highlights
          li=highlight