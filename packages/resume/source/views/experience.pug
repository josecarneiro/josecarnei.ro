- const experience = data.experience;
section.resume__section.resume__section--experience
  h2 Experience
  each item in experience
    .listing__item
      header.listing__header
        .listing__row
          h3=item.company
          small=functions.formatDate([ item.date_start, item.date_end ])
        if item.description
          small=item.description
      h4=item.position
      ul
        each highlight in item.highlights
          li=highlight